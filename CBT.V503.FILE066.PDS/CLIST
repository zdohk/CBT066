./        ADD LIST=ALL,NAME=INFO
PROC 1 KEYWORD
/* CPROC(INFO) - INFO KEYWORD SEARCH SIMULATOR
CLRSCRN
ALLOC FI(KWDS) DA('TCACF0.INFOSYS.TEXT($$$KWDS)') SHR REUSE
ALLOC FI(PDS)  DA('TCACF0.INFOSYS.TEXT') SHR REUSE
IF &KEYWORD = ALL THEN +
     CALL 'TCACF0.TEST.LOAD(INFO)'
ELSE +
     CALL 'TCACF0.TEST.LOAD(INFO)' '&KEYWORD'
FREE FI(KWDS,PDS)
ISPEXEC VGET ZUSER
IF &ZUSER = TCACF0 THEN +
   ISPEXEC EDIT DATASET('TCACF0.INFOSYS.TEXT')
ELSE +
   ISPEXEC BROWSE DATASET('TCACF0.INFOSYS.TEXT')
./        ADD LIST=ALL,NAME=TECHDISP
PROC 0 PRMS()
/* A CMDPROC TO PROVIDE A MENU OF ALL SORTS OF NIFTY DISPLAYS
CONTROL MAIN NOMSG NOLIST
DISPLAY: +
ISPEXEC DISPLAY PANEL(DISPLAY)
SELECT
   WHEN (&O =   0) GOTO FIN
   WHEN (&O =   1) ADMFINFO
   WHEN (&O =   2) CALL 'TCACF0.TEST.LOAD(APFLST)'
   WHEN (&O =   3) CALL 'TCACF0.TEST.LOAD(APPCVER)'
   WHEN (&O =   4) %ARCHLVL
   WHEN (&O =   5) CALL 'TCACF0.TEST.LOAD(ASCBGONE)'
   WHEN (&O =   6) CALL 'TCACF0.TEST.LOAD(ASCBSLOT)'
   WHEN (&O =   7) CALL 'TCACF0.TEST.LOAD(ASMVALS)'
   WHEN (&O =   8) CALL 'TCACF0.TEST.LOAD(ASVTSCAN)'
   WHEN (&O =   9) CACHE &PRMS
   WHEN (&O =  10) CALL 'TCACF0.TEST.LOAD(CATVOL)'
   WHEN (&O =  11) %CA1INFO
   WHEN (&O =  12) %CDSLVL
   WHEN (&O =  13) %PCHID &PRMS
   WHEN (&O =  14) CMCCHK
   WHEN (&O =  15) CALL 'TCACF0.TEST.LOAD(COLOURS)'
   WHEN (&O =  16) %COLORS
   WHEN (&O =  17) %CPU
   WHEN (&O =  18) %CPUSER
   WHEN (&O =  19) CALL 'TCACF0.TEST.LOAD(CSALST)'
   WHEN (&O =  20) DAL &PRMS
   WHEN (&O =  21) %ADYDSP0
   WHEN (&O =  22) %DAELIST
   WHEN (&O =  23) CALL 'TCACF0.TEST.LOAD(DCL)'
   WHEN (&O =  24) %DEVS &PRMS
   WHEN (&O =  25) +
     IF &SYSHSM  ¬= THEN WRITE DFHSM IS AVAILABLE, VER/REL/MOD = &SYSHSM
   WHEN (&O =  26) CALL 'TCACF0.TEST.LOAD(DFPLVL)' '&PRMS.'
   WHEN (&O =  27) CALL 'TCACF0.TEST.LOAD(DSFLVL)'
   WHEN (&O =  28) CALL 'TCACF0.TEST.LOAD(DSSLVL)'
   WHEN (&O =  29) %DSRM
   WHEN (&O =  30) DXCFSTR
   WHEN (&O =  31) DYNEXITS
   WHEN (&O =  32) %ISGACLS0
   WHEN (&O =  33) GUESTNAM
   WHEN (&O =  34) %INFO &PRMS
   WHEN (&O =  35) IOCDSLST
   WHEN (&O =  36) CALL 'TCACF0.TEST.LOAD(IOINFO)'
   WHEN (&O =  37) %IPLINFO
   WHEN (&O =  38) CALL 'TCACF0.TEST.LOAD(IPLMEM)'
   WHEN (&O =  39) CALL 'TCACF0.TEST.LOAD(IPLSTAT)'
   WHEN (&O =  40) ISRDDN
   WHEN (&O =  41) ISRFIND
   WHEN (&O =  42) %JES3JCT &PRMS
   WHEN (&O =  43) LASTCLPA
   WHEN (&O =  44) LASTIPL
   WHEN (&O =  45) LASTJES3
   WHEN (&O =  46) CALL 'TCACF0.TEST.LOAD(LELVL)'
   WHEN (&O =  47) LNKLST
   WHEN (&O =  48) LOOK
   WHEN (&O =  49) LPA
   WHEN (&O =  50) CALL 'TCACF0.TEST.LOAD(LPALST)'
   WHEN (&O =  51) CALL 'TCACF0.TEST.LOAD(LPARCAP)'
   WHEN (&O =  52) CALL 'TCACF0.TEST.LOAD(LPARNM)'
   WHEN (&O =  53) LXATSCAN
   WHEN (&O =  54) MAPPPT
   WHEN (&O =  55) MAPSYS &PRMS
   WHEN (&O =  56) CALL 'TCACF0.TEST.LOAD(MAPUCVT)'
   WHEN (&O =  57) %MAPXA
   WHEN (&O =  58) MEM
   WHEN (&O =  59) CALL 'TCACF0.TEST.LOAD(MULCCHK)'
   WHEN (&O =  60) %MSU
   WHEN (&O =  61) CALL 'TCACF0.TEST.LOAD(NUCMAP)'
   WHEN (&O =  62) CALL 'TCACF0.TEST.LOAD(OPTCHK)'
   WHEN (&O =  63) CALL 'TCACF0.TEST.LOAD(PARMLST)'
   WHEN (&O =  64) CALL 'TCACF0.TEST.LOAD(PROCINFO)'
   WHEN (&O =  65) %PRSMINFO
   WHEN (&O =  66) CALL 'TCACF0.TEST.LOAD(PUSE)'
   WHEN (&O =  67) CALL 'TCACF0.TEST.LOAD(RACFDS)'
   WHEN (&O =  68) +
          WRITE RACF IS &SYSRACF, VER/REL/MOD = &SYSLRACF
   WHEN (&O =  69) REALCPID
   WHEN (&O =  70) %RMFHIST
   WHEN (&O =  71) SHOWMVS
   WHEN (&O =  72) SHOWLPAR
   WHEN (&O =  73) CALL 'TCACF0.TEST.LOAD(SMFDS)'
   WHEN (&O =  74) %SORTOPT &PRMS
   WHEN (&O =  75) CALL 'TCACF0.TEST.LOAD(SPLEXNAM)'
   WHEN (&O =  76) SPY
   WHEN (&O =  77) CALL 'TCACF0.TEST.LOAD(SRMCONS)'
   WHEN (&O =  78) SSIDCHK
   WHEN (&O =  79) SSNAMES
   WHEN (&O =  80) SVCTAB &PRMS
   WHEN (&O =  81) LASTXCF
   WHEN (&O =  82) CALL 'TCACF0.TEST.LOAD(SYSSYM)'
   WHEN (&O =  83) SYSTAT
   WHEN (&O =  84) CALL 'TCACF0.TEST.LOAD(SWAQUERY)'
   WHEN (&O =  85) CALL 'TCACF0.TEST.LOAD(TAPETAB)'
   WHEN (&O =  86) TASID
   WHEN (&O =  87) +
          IF &SYSTSOE ¬= THEN WRITE TSO/E VER/REL/MOD = &SYSTSOE
   WHEN (&O =  88) ST TSO
   WHEN (&O =  89) CALL 'TCACF0.TEST.LOAD(FXCSCAN)'
   WHEN (&O =  90) %VSAMNAME
   WHEN (&O =  91) CALL 'TCACF0.TEST.LOAD(VTAMLVL)'
   WHEN (&O =  92) VTAMSESS &PRMS
   WHEN (&O =  93) CALL 'TCACF0.TEST.LOAD(WHATJES)'
   WHEN (&O =  94) CALL 'TCACF0.TEST.LOAD(WHATSEC)'
   WHEN (&O =  95) WHATSYS
   WHEN (&O =  96) CALL 'TCACF0.TEST.LOAD(WLMINFO)'
   WHEN (&O = 199) CALL 'TCACF0.TEST.LOAD(ILRBIRD)' 'BIRD'
   OTHERWISE GOTO FIN
END
SET &O =
GOTO DISPLAY
FIN: +
ERROR OFF
EXIT
