//SORT    EXEC PGM=SORT,REGION=8M
//SYSOUT   DD SYSOUT=*
//SORTIN   DD DISP=(OLD,PASS),DSN=&&LIST
//SORTOUT  DD DISP=(,PASS),DSN=&&M003,SPACE=(TRK,(10,1)),
// UNIT=VIO,RECFM=FB,LRECL=80,BLKSIZE=0,DSORG=PS
//SYSIN    DD *
 OMIT COND=(6,7,CH,EQ,C'M000003')
 INREC FIELDS=(1,80,
      18,3,CHANGE=(2,C'Jan',C'01',
                     C'Feb',C'02',
                     C'Mar',C'03',
                     C'Apr',C'04',
                     C'May',C'05',
                     C'Jun',C'06',
                     C'Jul',C'07',
                     C'Aug',C'08',
                     C'Sep',C'09',
                     C'Oct',C'10',
                     C'Nov',C'11',
                     C'Dec',C'12'))
 SORT FIELDS=(21,2,Y2C,D,            YY
              81,2,CH,D,             MM
              16,2,CH,D,             DD
              6,7,CH,A)              MNNNNNN
 OUTREC FIELDS=(6,74,6X)
