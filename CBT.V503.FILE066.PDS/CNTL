//KWDDATA  DD DISP=SHR,DSN=TCACF0.INFOSYS.TEXT($$$KWDS)
//SORTCNTL DD *
 OPTION DYNALLOC=OFF
//*
//*DEBUG1  EXEC PGM=ICEGENER,REGION=2M
//*SYSPRINT DD SYSOUT=*
//*SYSIN    DD DUMMY
//*SYSUT1   DD DISP=SHR,DSN=TCACF0.INFOSYS.TEXT($$$KWDS)
//*SYSUT2   DD SYSOUT=*
//*
//INFO2   EXEC PGM=INFO PARM='BLD'
//STEPLIB  DD DISP=SHR,DSN=TCACF0.TEST.LOAD
//SYSUDUMP DD SYSOUT=*
//SYSPRINT DD DISP=(,PASS),DSN=&&LIST,SPACE=(TRK,(10,1)),
// UNIT=VIO,RECFM=FB,LRECL=80,BLKSIZE=0,DSORG=PS
//PDS      DD DISP=SHR,DSN=TCACF0.INFOSYS.TEXT
//KWDS     DD DISP=SHR,DSN=TCACF0.INFOSYS.TEXT($$$KWDS)
//*
//*DEBUG2  EXEC PGM=ICEGENER,REGION=2M
//*SYSPRINT DD SYSOUT=*
//*SYSIN    DD DUMMY
//*SYSUT1   DD DISP=(OLD,PASS),DSN=&&LIST
//*SYSUT2   DD SYSOUT=*
